<body style="min-height:100%">
<table cellpadding="0" cellspacing="0" border="0" class="body">
<tr>
    <td height="30"></td>
</tr>
<tr>
<td class="center" align="center" valign="top">
<center>

<!-- Header -->

<table cellpadding="0" cellspacing="0" border="0" class="container header">
    <tr>
        <td class="last">

            <table cellpadding="0" cellspacing="0" border="0" class="twelve">
                <tr>
                    <td height="30"></td>
                </tr>
                <tr>
                    <td width="30"></td>
                    <td valign="middle">

                        <h1>НОВЫЕ МАТЕРИАЛЫ НА САЙТЕ <a href="#">NAVISTOM.COM</a></h1>

                        <div style="font-size:11px; color:#999; height:26px; line-height:40px">Рассылка
                            от {{ date|rusDate }}</div>
                    </td>
                    <td width="190" valign="middle" class="six sub-columns last"
                        style="text-align:right; vertical-align:middle;">

                        <a href="http://navistom.com">
                            <img width="154" height="41" title="NaviStom.com" alt="NaviStom.com" style="float:right"
                                 src="http://navistom.com/templates/Navistom/images/email-logo.png">
                        </a>

                    </td>
                    <td width="30"></td>
                </tr>
                <tr>
                    <td height="30"></td>
                </tr>
            </table>
        </td>
    </tr>
</table>

<!-- End header -->

<!-- Content -->

<table cellpadding="0" cellspacing="0" border="0" class="container content">
    <tr>
        <td width="30">&nbsp;</td>
        <td>

            {% for item in items %}

                {% if item.type == 'article' %}
                    <table cellpadding="0" cellspacing="0" border="0" width="100%" class="article-item">
                        <tr>
                            <td height="30"></td>
                        </tr>
                        <tr>
                            <td width="180" align="left">
                                <img width="150" height="100"
                                     src="http://navistom.com/uploads/images/articles/100x150/{{ item.image }}"/>
                            </td>
                            <td align="left">
                                <h2 style="line-height:20px;">
                                    <a href="http://navistom.com/articles/article/{{ item.content_id }}-{{ item.name|translit }}">{{ item.name }}</a>
                                </h2>

                                <p></p>
                                <span style="font-size:12px">{{ item.description }}</span>
                            </td>
                        </tr>
                        <tr>
                            <td height="30"></td>
                        </tr>
                        <tr>
                            <td colspan="3" bgcolor="#f1f0f0" height="1"></td>
                        </tr>
                    </table>
                {% else %}
                    <table cellpadding="0" cellspacing="0" border="0" width="100%" class="content-item">
                        <tr>
                            <td height="30"></td>
                        </tr>
                        <tr>
                            <td rowspan="2" width="140" align="left">
                                {% if item.image %}
                                    <img width="100" height="80"
                                         src="http://navistom.com/uploads/images/{{ item.image }}"/>
                                {% else %}
                                    <img width="100" height="80" src="http://navistom.com/uploads/images/100x80.jpg"/>
                                {% endif %}
                            </td>
                            {% if item.type == 'activity' %}
                                <td align="left">
                                    <h2>
                                        <a href="http://navistom.com/{{ item.type }}/{{ item.content_id }}-{{ item.name|translit }}">
                                            {{ item.name|raw }}, г. {{ item.currency_name }}
                                        </a>
                                    </h2>
                                    {% if item.description %}
                                        <span class="descr">{{ item.description }} {% if item.price %} - {{ item.price }} {% endif %}</span>
                                    {% endif %}
                                </td>
                                <td style="text-align:right" align="right">

                                </td>
                            {% else %}
                                <td align="left">
                                    <h2>
                                        <a href="http://navistom.com/{{ item.type }}/{{ item.content_id }}-{{ item.name|translit }}">{{ item.name|raw }}</a>
                                    </h2>
                                    {% if item.description %}
                                        <span class="descr">{{ item.description|raw }}</span>
                                    {% endif %}
                                </td>
                                <td style="text-align:right" align="right">
                                    {% if item.price %}
                                        <font style="font-size:13px"
                                              color="#f39130"><b>{{ item.price|number_format(0, '', ' ') }} {{ item.currency_name }}</b></font>
                                    {% endif %}
                                </td>
                            {% endif %}
                        </tr>
                        <tr>
                            <td height="15" style="vertical-align:bottom" valign="bottom">
                                <span style="font-size:12px">{{ item.user_name }}</span>
                            </td>
                            <td style="text-align:right" valign="bottom" align="right">
                                <a href="http://navistom.com/{{ item.type }}/{{ item.content_id }}-{{ item.name|translit }}">Перейти
                                    к объявлению</a>
                            </td>
                        </tr>
                        <tr>
                            <td height="30"></td>
                        </tr>
                        <tr>
                            <td colspan="3" bgcolor="#f1f0f0" height="1"></td>
                        </tr>
                    </table>
                {% endif %}
            {% endfor %}

        </td>
        <td width="30">&nbsp;</td>
    </tr>
